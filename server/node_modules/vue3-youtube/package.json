{
  "name": "vue3-youtube",
  "version": "0.1.9",
  "author": {
    "name": "Nikita Umnov",
    "email": "nomnes1990@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "youtube",
    "API",
    "iframe"
  ],
  "description": "Wrapper for YouTube IFrame Player API",
  "repository": {
    "type": "git",
    "url": "https://github.com/NomNes/vue3-youtube"
  },
  "main": "dist/vue3-youtube.common.js",
  "module": "dist/vue3-youtube.umd.js",
  "unpkg": "dist/vue3-youtube.umd.min.js",
  "jsdelivr": "dist/vue3-youtube.umd.min.js",
  "browser": "dist/vue3-youtube.umd.min.js",
  "typings": "dist/YouTube.vue.d.ts",
  "typescript": {
    "definition": "dist/YouTube.vue.d.ts"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib src/YouTube.vue && rm ./dist/demo.html ./dist/main.d.ts",
    "lint": "vue-cli-service lint",
    "version": "yarn version"
  },
  "dependencies": {
    "@types/youtube": "^0.0.39",
    "core-js": "^3.6.5",
    "get-youtube-id": "^1.0.1",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.7",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "typescript": "~3.9.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
